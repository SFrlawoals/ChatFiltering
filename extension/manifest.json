{
  "name": "Security Factorial Text Filtering Project",
  "version": "1.0",
  "description": "Security Factorial",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "background",
    "tabs",
    "https://*/*",
    "http://*/*"
    ],
  "browser_action": {
    "default_title": "Security Factorial!",
    "default_popup" : "popup/popup.html"
    },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
